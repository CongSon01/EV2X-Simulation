         x="7350" y="5500" layer="10"/>

    <!-- RSU marker (green dot) at position matching omnetpp.ini rsu[0] -->
    <poi id="RSU_0" type="rsu" color="0,200,0"
         x="7400" y="5800" layer="10"/>
