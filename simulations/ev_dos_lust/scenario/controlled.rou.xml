<!-- Controlled scenario: exactly 2 EVs on the LuST map.
     Routes use real LuST edges along the -30528 arterial (highway.primary).
     Both vehicles drive through the CS area (~7350,5500) and RSU area (~7400,5800).

     ev_attacker departs at t=10, ev_normal departs at t=20.
     In OMNeT++: ev_attacker = ev[0], ev_normal = ev[1] (order of creation).
     Colors are set via TraCI in VeinsInetEVChargingApp (red/yellow). -->

<routes>
    <!-- EV vehicle types: color is initial SUMO color (overridden by TraCI) -->
    <vType id="evAttacker" vClass="passenger" color="255,0,0"
           accel="2.6" decel="4.5" sigma="0.5" length="5.0" minGap="1.5"
           maxSpeed="50" speedDev="0.1" guiShape="passenger/sedan"/>

    <vType id="evNormal" vClass="passenger" color="255,255,0"
           accel="2.6" decel="4.5" sigma="0.5" length="5.0" minGap="1.5"
           maxSpeed="50" speedDev="0.1" guiShape="passenger/hatchback"/>

    <!-- Vehicle 1: Attacker (red) - long route through main arterial
         Route from real LuST vehicle randUni10684:1.
         Passes through -30528#4 to #42, covering both CS and RSU areas. -->
    <vehicle id="ev_attacker" type="evAttacker" depart="10.00"
             departPos="random" departSpeed="0.00"
             arrivalPos="random" arrivalSpeed="0.00">
        <route edges="-32694 --32058#0 -31852#1 -31910#1 -31910#2 --31768#0 -30924#1 -31372 -32830#1 --31940#6 --31940#5 --31940#4 --31940#3 --31940#2 --31940#1 --31940#0 --30256#3 --30256#2 --30256#1 --30256#0 --31834#2 --31834#1 --31834#0 -30528#4 -30528#5 -30528#6 -30528#7 -30528#8 -30528#9 -30528#10 -30528#11 -30528#12 -30528#13 -30528#14 -30528#15 -30528#16 -30528#17 -30528#18 -30528#19 -30528#20 -30528#21 -30528#22 -30528#23 -30528#24 -30528#25 -30528#26 -30528#27 -30528#28 -30528#29 -30528#30 -30528#31 -30528#32 -30528#33 -30528#34 -30528#35 -30528#36 -30528#37 -30528#38 -30528#39 -30528#40 -30528#41 -30528#42 --31794#7 --31794#6 --31794#5 --31794#4 -31884#0 -31884#1 -31884#2 -31884#3 -31884#4 -31034#0 -31034#1 -31034#2 -31034#3 -31034#4 --32756#2 --32756#1"/>
    </vehicle>

    <!-- Vehicle 2: Normal (yellow) - enters main road at -30528#21
         Route from real LuST vehicle randUni5299:1.
         Also passes through CS area (-30528#21 to #32). -->
    <vehicle id="ev_normal" type="evNormal" depart="20.00"
             departPos="random" departSpeed="0.00"
             arrivalPos="random" arrivalSpeed="0.00">
        <route edges="--32326#1 --31566 -30370#3 --31458#2 --31458#1 --31458#0 -31496#12 -31496#13 -31496#14 -32750#0 -32750#1 -32750#2 -32750#3 --32924#14 --32924#13 --32924#12 --32924#11 --32924#10 --32924#9 -30892#10 -30892#11 -30892#12 -30892#13 -30892#14 -30892#15 -30892#16 --31272#6 --31272#5 --31272#4 --31272#3 --31272#2 --31272#1 --31272#0 -30528#21 -30528#22 -30528#23 -30528#24 -30528#25 -30528#26 -30528#27 -30528#28 -30528#29 -30528#30 -30528#31 -30528#32 -30854#0 -30854#1 -30854#2 -31290#1 -31290#2 -31126#0 -32902#0 -32902#1"/>
